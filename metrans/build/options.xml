			<Width>1280</Width>
			<Height>720</Height>
			<VideoFilterDesc>hflip</VideoFilterDesc>
			<VideoEncParamSuffix>maxbitrate=2.5M</VideoEncParamSuffix>
			
			<OutputFormat>mp4</OutputFormat>
			<OutputFile>720p_#.mp4</OutputFile>
		</Resolution>
		<Resolution>
			<Width>960</Width>
			<Height>540</Height>
			<VideoFilterDesc>hflip</VideoFilterDesc>
			<VideoEncParamSuffix>maxbitrate=1.5M</VideoEncParamSuffix>
			
			<OutputFormat>mp4</OutputFormat>
			<OutputFile>540p_#.mp4</OutputFile>
		</Resolution>
	</Resolutions>
</Options>

